{
  "name": "graphql-eventbus-google-pubsub",
  "version": "0.0.25-alpha.0",
  "description": "Graphql eventbus for Google Pubsub",
  "main": "build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "PUBSUB_PROJECT_ID=123 PUBSUB_EMULATOR_HOST=localhost:8538 jest --runInBand --coverage --verbose --forceExit",
    "test:watch": "PUBSUB_PROJECT_ID=123 PUBSUB_EMULATOR_HOST=localhost:8538 jest --watch --runInBand"
  },
  "files": [
    "build/**/*"
  ],
  "dependencies": {
    "@google-cloud/pubsub": "^2.18.3",
    "graphql-eventbus": "latest",
    "module-alias": "^2.2.2",
    "uuid": "^8.3.2"
  },
  "author": "Suraj Keshri",
  "license": "MIT",
  "gitHead": "8ebfba031afcf75eb5a0aa498ec3c2476bb8ec4f"
}
