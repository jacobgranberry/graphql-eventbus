{
  "name": "graphql-eventbus-google-pubsub",
  "version": "0.0.23",
  "description": "Graphql eventbus for Google Pubsub",
  "main": "build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "test": "PUBSUB_PROJECT_ID=123 PUBSUB_EMULATOR_HOST=localhost:8538 jest  --runInBand --verbose --forceExit",
    "build": "rimraf build && tsc -p tsconfig.json",
    "prepublishOnly": "npm run build && npm version patch",
    "test:watch": "PUBSUB_PROJECT_ID=123 PUBSUB_EMULATOR_HOST=localhost:8538 npm run test -- --watch"
  },
  "files": [
    "build/**/*"
  ],
  "dependencies": {
    "@google-cloud/pubsub": "^2.18.3",
    "graphql-eventbus-core": "^0.0.27",
    "module-alias": "^2.2.2",
    "uuid": "^8.3.2"
  },
  "author": "Suraj Keshri",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^0.6.5",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.4"
  }
}
