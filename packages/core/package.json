{
  "name": "graphql-eventbus",
  "version": "0.0.36",
  "description": "Eventbus powered by GraphQL",
  "engines": {
    "node": ">= 14 <17"
  },
  "_moduleAliases": {
    "#root": "./build"
  },
  "main": "build/index.js",
  "files": [
    "build/**/*"
  ],
  "types": "./build/index.d.ts",
  "np": {
    "yarn": false,
    "contents": "build",
    "cleanup": false
  },
  "devDependencies": {
    "@graphql-tools/mock": "^8.4.3",
    "@types/uuid": "^8.3.0",
    "graphql-scalars": "^1.13.6"
  },
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "jest --runInBand --coverage --verbose --forceExit",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "author": "Suraj Keshri",
  "license": "MIT",
  "dependencies": {
    "@graphql-inspector/core": "^2.2.0",
    "@graphql-tools/schema": "^7.1.5",
    "module-alias": "^2.2.2",
    "ts-is-present": "^1.2.2",
    "uuid": "^8.3.2"
  }
}
