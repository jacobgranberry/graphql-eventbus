{
  "name": "graphql-eventbus",
  "version": "0.2.0",
  "description": "Eventbus powered by GraphQL",
  "engines": {
    "node": ">= 14 <17"
  },
  "homepage": "https://github.com/quant-daddy/graphql-eventbus",
  "bugs": {
    "url": "https://github.com/quant-daddy/graphql-eventbus/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quant-daddy/graphql-eventbus.git"
  },
  "keywords": [
    "graphql",
    "graphql-eventbus",
    "event"
  ],
  "_moduleAliases": {
    "#root": "./dist"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "np": {
    "yarn": false,
    "contents": "dist",
    "cleanup": false
  },
  "devDependencies": {
    "@graphql-tools/mock": "^8.6.8",
    "@types/uuid": "^8.3.0",
    "graphql-scalars": "^1.17.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "jest --runInBand --verbose --forceExit",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "author": "Suraj Keshri",
  "license": "MIT",
  "dependencies": {
    "@graphql-inspector/core": "^3.1.2",
    "@graphql-tools/schema": "^8.3.10",
    "module-alias": "^2.2.2",
    "ts-is-present": "^1.2.2",
    "uuid": "^8.3.2"
  }
}
